{
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"camelcase": 0,
		"consistent-return": 0,
		"eqeqeq": 0,
		"new-cap": 0,
		"no-shadow": 0,
		"no-trailing-spaces": 0,
		"no-use-before-define": [2, "nofunc"],
		"quotes": 0
	},
	"globals": {
		"GM_config": false,
		"GM_registerMenuCommand": false,
		"BezierEasing": false
	}
}
